
// Task 11 and 12


#include <stdio.h>

int main(void)
{
    char str[] = "Hello world!";
    unsigned int counter = 0;

    for (int i = 0; i < sizeof(str); i++)
    {
    if(str[i]=='l'){
        str[i]='x';
    }
        printf("%c", str[i]);
    }

    printf(" = ");
    for (int i = sizeof(str); i >= 0; i--)
    {
    
        printf("%c", str[i]);
    }
    return 0;
}
