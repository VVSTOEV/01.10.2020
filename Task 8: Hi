// Task 8

#include <stdio.h>

int main(void)
{

    char s[] = "Hello everybody!";
    unsigned int counter = 0;

    for (int i = 0; i < sizeof(s); i++)
    {
        printf("%c", s[i]);
        if (s[i] == ' ')
        {
            continue;
        }
        else
        {
            counter++;
        }
    }

    printf("\nTotal letters is: %d", counter - 1);

    return 0;
}
